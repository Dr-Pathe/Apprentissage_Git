     "output_type": "stream",
     "text": [
      "Enter a number: 2\n",
      "the divasors of 2 are: \n",
      "2\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "print(\"the divasors of\", num, \"are: \")\n",
    "\n",
    "for i in range(2, num+1):\n",
    "    if num % i==0:\n",
    "        print(i)"
   ]
